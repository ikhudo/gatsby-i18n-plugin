{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "lerna run build --ignore gatsby-starter*",
    "build:watch": "lerna run build:watch --parallel --ignore gatsby-starter*",
    "deploy": "lerna run deploy --concurrency 1 --scope gatsby-starter*",
    "format": "prettier --config ./.prettierrc --write '**/*.js'"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.8.2",
    "gh-pages": "^2.0.1",
    "jest": "^23.4.2",
    "lerna": "^3.0.2",
    "prettier": "^1.13.7"
  },
  "workspaces": [
    "packages/*",
    "starters/*"
  ]
}
