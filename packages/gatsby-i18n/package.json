{
  "name": "@wework/gatsby-i18n",
  "description": "Gatsby plugin that provides i18n support",
  "version": "1.2.0",
  "repository": "WeConnect/gatsby-i18n-plugin",
  "keywords": [
    "react",
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "lingui",
    "i18n"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "/lib",
    "plugin.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib --ignore __tests__",
    "build:watch": "rimraf lib && babel -w src --out-dir lib --ignore __tests__",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@wapps/langtag-utils": "^1.0.2",
    "react-helmet": "^5.2.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-core": "^6.4.0",
    "gatsby": "^2.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rimraf": "^2.6.2"
  }
}
